{
  "name": "rich-slate",
  "version": "0.4.0",
  "license": "MIT",
  "description": "Opinionated rich text editor built on top of slate",
  "author": "ahmed osama",
  "typings": "dist/main.d.ts",
  "main": "dist/main.cjs.js",
  "module": "dist/main.esm.js",
  "exports": {
    ".": {
      "require": "./dist/main.cjs.js",
      "default": "./dist/main.esm.js"
    },
    "./tailwind": "./dist/tailwind.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedosama7450/rich-slate.git"
  },
  "bugs": {
    "url": "https://github.com/ahmedosama7450/rich-slate/issues"
  },
  "homepage": "https://github.com/ahmedosama7450/rich-slate#readme",
  "keywords": [
    "rich text editor"
  ],
  "scripts": {
    "build": "del-cli --force ./dist && rollup -c",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs --docs",
    "gen-barrels": "barrelsby --config ./barrels.json --delete",
    "prepare": "husky install",
    "publish-local": "npm run build && yalc push",
    "publish-local:watch": "rollup -c --watch --watch.onEnd=\"yalc push\"",
    "link:overwind-ui": "yalc add overwind-ui && yalc link overwind-ui && npm install",
    "unlink:overwind-ui": "yalc remove overwind-ui && npm install"
  },
  "dependencies": {
    "escape-goat": "^4.0.0",
    "is-url-superb": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@headlessui/react": "^1.5.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/classnames": "^2.3.1",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "barrelsby": "^2.3.4",
    "classnames": "^2.3.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.3",
    "eslint-plugin-storybook": "^0.6.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next": "^12.2.3",
    "overwind-ui": "0.35.1",
    "postcss": "^8.4.16",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "rollup": "^2.71.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "slate": "^0.82.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.82.0",
    "storybook-addon-next-router": "^4.0.0",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.5.0",
    "classnames": "^2",
    "next": "^12",
    "overwind-ui": "*",
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18",
    "react-hook-form": "^7",
    "slate": "^0.82.1",
    "slate-history": "^0.66.0",
    "slate-react": "^0.82.1"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": "eslint --cache --fix",
    "*.{ts,js,tsx,jsx,html,css,md}": "prettier --write"
  }
}
